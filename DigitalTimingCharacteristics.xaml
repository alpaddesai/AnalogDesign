<Window x:Class="AnalogDesign.DigitalTimingCharacteristics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnalogDesign"
        mc:Ignorable="d"
        Title="DigitalTimingCharacteristics" Height="450" Width="800" MaxWidth="800" MaxHeight="450">
    <Grid Margin="0,0,-8,0">
        <Image HorizontalAlignment="Left" Height="190" Margin="76,83,0,0" VerticalAlignment="Top" Width="240" RenderTransformOrigin="0.215,0.876">
            <Image.OpacityMask>
                <ImageBrush/>
            </Image.OpacityMask>
        </Image>
        <Image Margin="-6,8,369,11" Source="DigitalCharacteristics.jpg" Stretch="Fill" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.405"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-1.493"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image Margin="399,10,10,10" Source="DigitalCharacteristicsPNP.jpg" Stretch="Fill"/>

    </Grid>
</Window>
